; This is for testing local variables

; Obviously you can just xor here, this is just to test locals.


~void:switch {
	~int:tmp = 0
	move~ r1, tmp
	move~ r2, r1
	move~ tmp, r2
}

~int:main {
	~string:testLocal = "I am the Walrus\n"
	move~ testLocal, r1
	call~ printf

	~int:a = 5
	~int:b = 2
	move~ a, r1
	move~ b, r2
	call~ switch

	~string:sw = "a: %ld, b: %ld\n"
	move~ r2, r3
	move~ r1, r2
	move~ sw, r1
	call~ printf

	return~ 0
}
